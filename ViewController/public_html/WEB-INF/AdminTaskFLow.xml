<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="AdminTaskFLow">
    <default-activity>admin</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="admin">
      <page>/AdminFlows/admin.jsf</page>
    </view>
    <view id="AdminNext">
      <page>/AdminFlows/AdminNext.jsf</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>admin</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>next</from-outcome>
        <to-activity-id>AdminNext</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
